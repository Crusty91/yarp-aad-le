{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "minimumroute": {
        "ClusterId": "minimumcluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },

    "Clusters": {
      "minimumcluster": {
        "Destinations": {
          "backend": {
            "Address": "https://github.com/"
          }
        }
      }
    }
  }
}
